   setenv applications "$limitedbuild_applications"
else # complementary buildandinstall, legacy tools (geocase based)
   setenv applications "$complementarybuild_applications"
endif
setenv all_applications "$limitedbuild_applications $complementarybuild_applications"
echo
echo "applications are: "
echo $applications
echo

